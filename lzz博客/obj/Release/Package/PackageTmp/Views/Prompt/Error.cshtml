@model lzz博客.Models.Error

@{
    ViewBag.Title = "发生错误";
    Layout = "~/Views/Shared/_Prompt.cshtml";
}

<div class="row">
    
        <div class="col-xs-3">@Html.DisplayFor(model => model.Title)</div>
            <div class="col-xs-3">
                @Html.Raw(Model.Details)</div>
                <div class="col-xs-3">
                    @Html.Raw(Model.Cause)</div>
                    <div class="col-xs-3">
                        @Html.Raw(Model.Solution)</div>
                </div>


